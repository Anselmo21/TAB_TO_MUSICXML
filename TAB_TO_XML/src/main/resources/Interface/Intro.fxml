<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Interface.Controller">
   <children>
      <AnchorPane layoutX="-15.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="650.0" prefWidth="950.0" stylesheets="@Intro.css">
         <children>
            <TextArea fx:id="write" layoutX="25.0" layoutY="75.0" prefHeight="455.0" prefWidth="445.0" promptText="Paste a tablature here, or edit a browsed tablature!">
               <font>
                  <Font name="Consolas Bold" size="14.0" />
               </font></TextArea>
            <Button fx:id="convert" layoutX="818.0" layoutY="562.0" mnemonicParsing="false" onAction="#handleButtonConvert" text="Convert File" />
            <Label layoutX="227.0" layoutY="35.0" prefHeight="40.0" prefWidth="304.0" text="Browse using the button below" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Bookman Old Style Bold" size="18.0" />
               </font>
            </Label>
          <Button fx:id="browse" layoutX="25.0" layoutY="562.0" mnemonicParsing="false" onAction="#handleButtonBrowse" text="Browse File" />
            <Label fx:id="path" layoutX="25.0" layoutY="599.0" prefHeight="62.0" prefWidth="435.0" textFill="WHITE" wrapText="true" AnchorPane.bottomAnchor="39.0" AnchorPane.leftAnchor="25.0" AnchorPane.rightAnchor="490.0" AnchorPane.topAnchor="599.0" />
            <Button fx:id="save" layoutX="840.0" layoutY="628.0" mnemonicParsing="false" onAction="#handleButtonSave" text="Save File" />
            <TextArea fx:id="view" editable="false" layoutX="483.0" layoutY="75.0" prefHeight="455.0" prefWidth="446.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
