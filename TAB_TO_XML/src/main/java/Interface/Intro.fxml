            </Label>
          <Button fx:id="browse" layoutX="25.0" layoutY="500.0" mnemonicParsing="false" onAction="#handleButtonBrowse" text="Browse" />
            <Label fx:id="path" layoutX="113.0" layoutY="457.0" prefHeight="111.0" prefWidth="335.0" textFill="WHITE" wrapText="true" />
         </children>
      </AnchorPane>
   </children>
